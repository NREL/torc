{
  jobs: [
    {
      name: "preprocess",
      command: "python tests/scripts/preprocess.py -i output/inputs.json -o output/f1.json",
      user_data: [],
      cancel_on_blocking_job_failure: true,
      supports_termination: false,
      scheduler: "slurm_schedulers/debug",
      resource_requirements: "small",
      input_files: [
        "inputs"
      ],
      output_files: [
        "file1"
      ],
      blocked_by: []
    },
    {
      name: "work1",
      command: "python tests/scripts/work.py -i output/f1.json -o output/f2.json",
      user_data: [
        {
          key1: "val1"
        }
      ],
      cancel_on_blocking_job_failure: true,
      supports_termination: false,
      scheduler: "slurm_schedulers/debug",
      resource_requirements: "medium",
      input_files: [
        "file1"
      ],
      output_files: [
        "file2"
      ],
      blocked_by: [
        "preprocess"
      ]
    },
    {
      name: "work2",
      command: "python tests/scripts/work.py -i output/f1.json -o output/f3.json",
      user_data: [],
      cancel_on_blocking_job_failure: true,
      supports_termination: false,
      scheduler: "slurm_schedulers/debug",
      resource_requirements: "large",
      input_files: [
        "file1"
      ],
      output_files: [
        "file3"
      ],
      blocked_by: [
        "preprocess"
      ]
    },
    {
      name: "postprocess",
      command: "python tests/scripts/postprocess.py -i output/f2.json -i output/f3.json -o output/f4.json",
      user_data: [],
      cancel_on_blocking_job_failure: true,
      supports_termination: false,
      scheduler: "slurm_schedulers/debug",
      resource_requirements: "small",
      input_files: [
        "file3",
        "file2"
      ],
      output_files: [
        "file4"
      ],
      blocked_by: [
        "work2",
        "work1"
      ]
    }
  ],
  files: [
    {
      name: "inputs",
      path: "output/inputs.json"
    },
    {
      name: "file1",
      path: "output/f1.json"
    },
    {
      name: "file2",
      path: "output/f2.json"
    },
    {
      name: "file3",
      path: "output/f3.json"
    },
    {
      name: "file4",
      path: "output/f4.json"
    }
  ],
  resource_requirements: [
    {
      name: "small",
      num_cpus: 1,
      num_gpus: 0,
      num_nodes: 1,
      memory: "1g",
      runtime: "P0DT5M"
    },
    {
      name: "medium",
      num_cpus: 4,
      num_gpus: 0,
      num_nodes: 1,
      memory: "8g",
      runtime: "P0DT10M"
    },
    {
      name: "large",
      num_cpus: 8,
      num_gpus: 0,
      num_nodes: 1,
      memory: "16g",
      runtime: "P0DT20M"
    }
  ],
  schedulers: {
    slurm_schedulers: [
      {
        name: "debug",
        account: "my_account",
        nodes: 1,
        partition: "debug",
        qos: "standby",
        walltime: "01:00:00",
      }
    ],
  },
  config: {
    compute_node_resource_stats: {
      cpu: true,
      disk: false,
      memory: true,
      network: false,
      process: true,
      include_child_processes: true,
      recurse_child_processes: false,
      monitor_type: "periodic",
      make_plots: true,
      interval: 1
    }
  }
}
